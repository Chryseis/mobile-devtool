{
  "name": "mobile-devtool",
  "version": "0.1.0",
  "private": true,
  "main": "dist/main.js",
  "dependencies": {
    "@ant-design/icons": "^5.2.6",
    "@testing-library/jest-dom": "^5.17.0",
    "@testing-library/react": "^13.4.0",
    "@testing-library/user-event": "^13.5.0",
    "@types/jest": "^27.5.2",
    "@types/node": "^16.18.60",
    "@types/react": "^  18.2.33",
    "@types/react-dom": "^18.2.14",
    "antd": "^5.11.1",
    "electron": "^27.0.2",
    "electron-is-dev": "^2.0.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-router-dom": "^6.18.0",
    "styled-components": "^6.1.0",
    "typescript": "^4.9.5"
  },
  "scripts": {
    "start": "craco start",
    "build": "craco build",
    "dev": "concurrently -k \"yarn start\" \"yarn build:main\" \"yarn electron\"",
    "build:main": "tsc -w -p tsconfig.electron.json",
    "electron": "wait-on tcp:3000 && nodemon --exec electron ."
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@craco/craco": "^7.1.0",
    "concurrently": "^8.2.2",
    "nodemon": "^3.0.1",
    "prettier": "^3.0.3",
    "react-scripts": "^5.0.1",
    "wait-on": "^7.0.1"
  }
}
